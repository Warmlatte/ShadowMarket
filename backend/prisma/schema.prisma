generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Item {
  id         Int      @id @default(autoincrement())
  name       String   @unique(map: "name_UNIQUE")
  type       String
  effect     String
  price      Float
  weight     Float
  rarity     String
  detail_url String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
